    <!-- http://localhost/chemtop#AcyclicAliphaticHydrocarbon -->

    <owl:Class rdf:about="#AcyclicAliphaticHydrocarbon">
        <rdfs:label>aliphatic chain</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Hydrocarbon"/>
        <skos:definition
            >Hydrocarbon without rings.</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#Adenin-9-yl_group -->

    <owl:Class rdf:about="#Adenin-9-yl_group">
        <rdfs:label>adenine</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PurineGroup"/>
        <rdfs:comment>C5H5N5</rdfs:comment>
    </owl:Class>
    


    <!-- http://localhost/chemtop#AminoAcidChain -->

    <owl:Class rdf:about="#AminoAcidChain">
        <rdfs:label
            >amino acid sequence</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#AminoAcidResidue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom rdf:resource="#AminoAcidResidue"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <skos:definition
            >Chain of amino acids, joined by peptide bonds. Folded or unfolded.</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#AminoAcidMolecule -->

    <owl:Class rdf:about="#AminoAcidMolecule">
        <rdfs:label
            >amino acid monomer</rdfs:label>
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#AminoGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#CarboxyGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#condensesTo"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) ro:hasPart some CarbonHydrogenSubstructure and ((hasComponentPart myx 2 CarboxyGroup) and (hasComponentPart min 1 CarboxyGroup) and (hasComponentPart exactly 1 AminoGroup)) or ((hasComponentPart max 2 AminoGroup) and (hasComponentPart min 1 AminoGroup) and (hasComponentPart exactly 1 CarboxyGroup))</rdfs:comment>
        <skos:definition
            >In chemistry, an amino acid is any molecule that contains both amine and carboxyl functional groups. However, in biochemistry, this more general term is frequently used to refer to alpha amino acids (amino acids in which the amino and carboxylate functionalities are attached to the same carbon, called &#945;&#8211;carbon). Amino Acids have at least one amino and one carboxy group. Those having two carboxy groups have only one amino group and vice versa.</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#AminoAcidResidue -->

    <owl:Class rdf:about="#AminoAcidResidue">
        <rdfs:subClassOf rdf:resource="#Residue"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#boundedBy"/>
                <owl:someValuesFrom rdf:resource="#PeptideBoundary"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://localhost/chemtop#AminoGroup -->

    <owl:Class rdf:about="#AminoGroup">
        <rdfs:label>amino group</rdfs:label>
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#HydrogenAtom"/>
                            <rdf:Description rdf:about="#NitrogenAtom"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#NitrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 NitrogenAtom and hasComponentPart max 3 HydrogenAtom and hasComponentPart min 1 HydrogenAtom and hasComponentPart only (NitrogenAtom or HydrogenAtom)</rdfs:comment>
        <skos:definition
            >Amines are organic compounds and a type of functional group that contain nitrogen as the key atom. Isolated neutral or kationic amino group or amino group forming a peptide bond. NH2 or NH3+</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#AromaticHydrocarbon -->

    <owl:Class rdf:about="#AromaticHydrocarbon">
        <rdfs:label
            >aromatic hydrocarbon</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Hydrocarbon"/>
        <skos:definition
            >Hydrocarbons which contain aromatic rings (benzene, naphtalin, toluene)</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#Atom -->

    <owl:Class rdf:about="#Atom">
        <rdfs:label>atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MaterialEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom rdf:resource="#SubatomicParticle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#SubatomicParticle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition
            >Basic building block of molecules. Particle of discrete types. Biologically important atoms are C, N, H, O, P, S, Na, Ca, K, Mg, Cl, J, Fe, Co, Zn ,... Atoms may be electrically neutral or ionic.</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#BackbonePhosphateUnit -->

    <owl:Class rdf:about="#BackbonePhosphateUnit">
        <rdfs:label>phosphate</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MolecularPart"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#OxygenAtom"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#PhosphorusAtom"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#OxygenAtom"/>
                                    <rdf:Description rdf:about="#PhosphorusAtom"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <skos:definition
            >A backbone phosphate unit consists of a phosphorus atom bound to two oxygene atoms +++</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#BackbonePhosphateUnitBoundary -->

    <owl:Class rdf:about="#BackbonePhosphateUnitBoundary">
        <rdfs:subClassOf rdf:resource="#MolecularPartBoundary"/>
    </owl:Class>
    


    <!-- http://localhost/chemtop#C10-24Chain -->

    <owl:Class rdf:about="#C10-24Chain">
        <rdfs:label>C10-24 chain</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CarbonChain"/>
    </owl:Class>
    


    <!-- http://localhost/chemtop#C5Chain -->

    <owl:Class rdf:about="#C5Chain">
        <rdfs:label>C5 chain</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CarbonChain"/>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 5 CarbonAtom</rdfs:comment>
        <skos:definition
            >Backbone of Pentoses.</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#C6Chain -->

    <owl:Class rdf:about="#C6Chain">
        <rdfs:label>C6 chain</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CarbonChain"/>
        <skos:definition
            >Backbone of Hexoses.</skos:definition>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 6 CarbonAtom</rdfs:comment>
    </owl:Class>
    


    <!-- http://localhost/chemtop#Carbohydrate -->

    <owl:Class rdf:about="#Carbohydrate">
        <rdfs:label>carbohydrate</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Monosaccharide"/>
                    <rdf:Description rdf:about="#Oligosaccharide"/>
                    <rdf:Description rdf:about="#Polysaccharide"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
    </owl:Class>
    


    <!-- http://localhost/chemtop#CarbonAtom -->

    <owl:Class rdf:about="#CarbonAtom">
        <rdfs:label>carbon atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://localhost/chemtop#CarbonChain -->

    <owl:Class rdf:about="#CarbonChain">
        <rdfs:label>carbon chain</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#CarbonAtom"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom rdf:resource="#CarbonAtom"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#StructuralGroup"/>
    </owl:Class>
    


    <!-- http://localhost/chemtop#CarbonHydrogenSubstructure -->

    <owl:Class rdf:about="#CarbonHydrogenSubstructure">
        <rdfs:label
            >carbon hydrogen substructure</rdfs:label>
        <rdfs:subClassOf rdf:resource="#StructuralGroup"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#CarbonAtom"/>
                            <rdf:Description rdf:about="#HydrogenAtom"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 CarbonAtom and hasComponentPart exactly 1 HydrogenAtom and hasComponentPart only (CarbonAtom or HydrogenAtom)</rdfs:comment>
        <skos:definition
            >Part of a molecule constituted by a (1-3 valued) carbon hydrogen bound. Traditional (fiat) criterion for distinguishing organic from inorganic compounds. Excludes certain carbon compounds such as HCN, CO2, traditionally considered inorganic. (Could be enhanced by carbon tetrahalogen, traditionally considered organic compounds but irrelevant for biology)</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#CarbonylGroup -->

    <owl:Class rdf:about="#CarbonylGroup">
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
    </owl:Class>
    


    <!-- http://localhost/chemtop#CarboxyGroup -->

    <owl:Class rdf:about="#CarboxyGroup">
        <rdfs:label>carboxy group</rdfs:label>
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#OxygenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#CarbonAtom"/>
                            <rdf:Description rdf:about="#HydrogenAtom"/>
                            <rdf:Description rdf:about="#OxygenAtom"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 CarbonAtom and hasComponentPart exactly 2 OxygenAtom and hasComponentPart max 1 HydrogenAtom and hasComponentPart only (CarbonAtom or OxygenAtom or HydrogenAtom)</rdfs:comment>
        <skos:definition>COOH or COO-</skos:definition>
    </owl:Class>
    


    <!-- http://localhost/chemtop#ChromosomalDNA -->

    <owl:Class rdf:about="#ChromosomalDNA">
        <rdfs:label>chromosomal DNA</rdfs:label>
        <rdfs:subClassOf rdf:resource="#DNAMolecule"/>
        <skos:definition
            >Single large macromolecule of DNA that in its coiled form represents a chromosome</skos:definition>
        <owl:versionInfo
            >TODO: Discuss whether only complete DNA molecules can be ChromosomalDNA.</owl:versionInfo>
    </owl:Class>
    


