    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#NucleicAcidRegion -->

    <owl:Class rdf:about="#NucleicAcidRegion">
        <rdfs:label xml:lang="en"
            >nucleic acid region</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#NucleosidylGroup -->

    <owl:Class rdf:about="#NucleosidylGroup">
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HeterocyclicBaseGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#SugarRing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Nucleotide -->

    <owl:Class rdf:about="#Nucleotide">
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#condensesTo"/>
                <owl:someValuesFrom rdf:resource="#NucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#PhosphateGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HeterocyclicBaseGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#SugarRing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#NucleotideResidue -->

    <owl:Class rdf:about="#NucleotideResidue">
        <rdfs:label xml:lang="en">nucleotide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Residue"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#BackbonePhosphateUnit"/>
                            <rdf:Description rdf:about="#HeterocyclicBaseGroup"/>
                            <rdf:Description rdf:about="#SugarRing"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) hasComponentPart exactly 1 HeterocyclicBase and hasComponentPart exactly 1 Phosphate and hasComponentPart only (Phosphate or Ribose or HeterocyclicBase) and hasComponentPart exactly 1 Pentose</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Oligomer -->

    <owl:Class rdf:about="#Oligomer">
        <rdfs:label xml:lang="en"
            >oligo or polymer</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#OrganicMolecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom rdf:resource="#Residue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:onClass rdf:resource="#Residue"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: A Oligomer is a molecule of low relative molecular mass of which the components can be fully specified. The components are all of the same kind, e.g. amino acid residues or nucleotide residues.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#OligopeptideMolecule -->

    <owl:Class rdf:about="#OligopeptideMolecule">
        <rdfs:label xml:lang="en"
            >entire oligopeptide molecule</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Peptide"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom rdf:resource="#AminoAcidResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:onClass rdf:resource="#AminoAcidResidue"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) hasComponentPart max 10 AminoAcidMonomer</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Amino Acid Polymer up to 10 monomers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Oligosaccharide -->

    <owl:Class rdf:about="#Oligosaccharide">
        <rdfs:label xml:lang="en">oligosaccharide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Carbohydrate"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) Carbohydrate and hasComponentPart min 2 Monosaccharide and hasComponentPart max 9 Monosaccharide and hasComponentPart only Monomer</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#OrganicMolecule -->

    <owl:Class rdf:about="#OrganicMolecule">
        <rdfs:label xml:lang="en"
            >organic molecular entity</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Molecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasProperPart"/>
                        <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Substances containing carbon and its derivatives as their prinicipal elements, usually combined with itself and with atoms of one or more other element such as hydrogen, oxygen, nitrogen, sulfur, phosphorus, chlorine or fluorine. They exist in either carbon chain or carbon ring form. Excludes carbides, carbonates and carbon oxides.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#OxygenAtom -->

    <owl:Class rdf:about="#OxygenAtom">
        <rdfs:label xml:lang="en">oxygen atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Pentose -->

    <owl:Class rdf:about="#Pentose">
        <rdfs:label xml:lang="en">pentose</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Monosaccharide"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#HydroxyGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) hasComponentPart exactly 1 C5Chain</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Peptide -->

    <owl:Class rdf:about="#Peptide">
        <rdfs:label xml:lang="en">peptide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: We here use the term peptide in the most general sense, viz. a oligo or polymer made out of amino acids.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PeptideBoundary -->

    <owl:Class rdf:about="#PeptideBoundary">
        <rdfs:subClassOf rdf:resource="#MolecularPartBoundary"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PhosphateGroup -->

    <owl:Class rdf:about="#PhosphateGroup">
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Phospholipid -->

    <owl:Class rdf:about="#Phospholipid">
        <rdfs:subClassOf rdf:resource="#Lipid"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PhosphorusAtom -->

    <owl:Class rdf:about="#PhosphorusAtom">
        <rdfs:label xml:lang="en">phosphorus atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PhysicalBoundary -->

    <owl:Class rdf:about="#PhysicalBoundary">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A independent continuant entity that bounds or demarcates some other continuant entity. Commonly viewed as a lower-dimensional part, the the notion of physical boundary here also encompasses ill-delimited spaces that fulfill the same function, e.g. biological joints.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Polymer -->

    <owl:Class rdf:about="#Polymer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#OrganicMolecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:allValuesFrom rdf:resource="#Residue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:onClass rdf:resource="#Residue"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PolypeptideMolecule -->

    <owl:Class rdf:about="#PolypeptideMolecule">
        <rdfs:label xml:lang="en"
            >entire polypeptide molecule</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Peptide"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:allValuesFrom rdf:resource="#AminoAcidResidue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:onClass rdf:resource="#AminoAcidResidue"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) Peptide and hasComponentPart min 11 AminoAcidMonomer and not (ro:properPartOf some MolecularEntity) and hasComponentPart only AminoAcidMonomer and hasComponentPart max 100 AminoAcidMonomer</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Amino Acid Polymer from 11 to 100 monomers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Polysaccharide -->

    <owl:Class rdf:about="#Polysaccharide">
        <rdfs:label xml:lang="en">polysaccharide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Carbohydrate"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) Carbohydrate and hasComponentPart min 10 Monosaccharide and hasComponentPart only Monomer</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PortionOfHeterogeneousLiquid -->

    <owl:Class rdf:about="#PortionOfHeterogeneousLiquid">
        <rdfs:label xml:lang="en"
            >portion of heterogenous liquid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CompoundOfCollectiveMaterialEntities"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Liquid constituted by different kinds of collectives, e.g. aqueous NaCl solution</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PortionOfHeterogeneousSolid -->

    <owl:Class rdf:about="#PortionOfHeterogeneousSolid">
        <rdfs:label xml:lang="en"
            >portion of heterogenous solid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CompoundOfCollectiveMaterialEntities"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Solid constituted by different kinds of collectives, e.g. a NaCl crystal, granite</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PortionOfHomogeneousGas -->

    <owl:Class rdf:about="#PortionOfHomogeneousGas">
        <rdfs:label xml:lang="en">portion of gas</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PortionOfHomogeneousMatter"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Non-connected  Molecules in gaseous state</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PortionOfHomogeneousLiquid -->

    <owl:Class rdf:about="#PortionOfHomogeneousLiquid">
        <rdfs:label xml:lang="en"
            >portion of homogenous liquid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PortionOfHomogeneousMatter"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Pure liquid consisting only of one kind of particles, e.g. H2O, liquid Hg, ...</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PortionOfHomogeneousMatter -->

    <owl:Class rdf:about="#PortionOfHomogeneousMatter">
        <rdfs:label xml:lang="en"
            >homogenous cohesive collection</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CollectiveMaterialEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasGranularPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Atom"/>
                            <rdf:Description rdf:about="#Molecule"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasGranularPart"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Atom"/>
                            <rdf:Description rdf:about="#Molecule"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Composition of granular components of the same kind.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PortionOfHomogeneousSolid -->

    <owl:Class rdf:about="#PortionOfHomogeneousSolid">
        <rdfs:label xml:lang="en"
            >portion of homogenous solid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PortionOfHomogeneousMatter"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Pure solid consisting only of one kind of particles, e.g. a piece of metal</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#ProteinComplex -->

    <owl:Class rdf:about="#ProteinComplex">
        <rdfs:label xml:lang="en">protein complex</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MoleculeComplex"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:onClass rdf:resource="#ProteinSubunit"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Mostly only proteins but composition with other molecules possible (e.g. Fe -&gt; Hemoglobin)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#ProteinDomain -->

    <owl:Class rdf:about="#ProteinDomain">
        <rdfs:label xml:lang="en">protein domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidChain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#partOf"/>
                <owl:someValuesFrom rdf:resource="#ProteinMolecule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Structural domain is the smallest element of protein tertiary structure that is self-stabilizing and often folds independently of the rest of the amino acid chain. It may contain one or more structural motives (ProteinSubstructure). A protein domain typically has its own function, the overall function of the protein results from the composition of the functions of all of its structural domains.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#ProteinMolecule -->

    <owl:Class rdf:about="#ProteinMolecule">
        <rdfs:label xml:lang="en"
            >entire protein molecule</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PolypeptideMolecule"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) Peptide and hasComponentPart min 101 AminoAcidMonomer and not (ro:properPartOf some MolecularEntity)</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Amino Acid Polymer longer than 100 monomers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#ProteinSubstructure -->

    <owl:Class rdf:about="#ProteinSubstructure">
        <rdfs:label xml:lang="en"
            >protein substructure</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#partOf"/>
                <owl:someValuesFrom rdf:resource="#ProteinMolecule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidChain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: A secondary substructure of a protein (protein Motif)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#ProteinSubunit -->

    <owl:Class rdf:about="#ProteinSubunit">
        <rdfs:label xml:lang="en">protein subunit</rdfs:label>
        <rdfs:subClassOf rdf:resource="#ProteinMolecule"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: A protein molecule that assembles with other protein molecules to form a protein complex. May have its own function. However, most proteinsubunits do not gain functionality until associated with the other subunits.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Proton -->

    <owl:Class rdf:about="#Proton">
        <rdfs:label xml:lang="en">proton</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SubatomicParticle"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Positively Charged Particle</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PurineGroup -->

    <owl:Class rdf:about="#PurineGroup">
        <rdfs:subClassOf rdf:resource="#HeterocyclicBaseGroup"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#PyrimidineGroup -->

    <owl:Class rdf:about="#PyrimidineGroup">
        <rdfs:subClassOf rdf:resource="#HeterocyclicBaseGroup"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#RNAMolecule -->

    <owl:Class rdf:about="#RNAMolecule">
        <rdfs:label rdf:datatype="&xsd;string">RNA sequence</rdfs:label>
        <rdfs:label xml:lang="en">RNA</rdfs:label>
        <rdfs:subClassOf rdf:resource="#NucleicAcidMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#RibonucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >any kind of NucleicAcid that is composed of Ribonucleotides which are not Deoxyribonucleotides</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#RNARegion -->

    <owl:Class rdf:about="#RNARegion">
        <rdfs:label xml:lang="en">RNA region</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#RibonucleotideResidue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#properPartOf"/>
                        <owl:someValuesFrom rdf:resource="#RNAMolecule"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom rdf:resource="#RibonucleotideResidue"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#NucleicAcidRegion"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Any properPartOf an EntireRNAMolecule that as only ribonucleotides as components.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Residue -->

    <owl:Class rdf:about="#Residue">
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Ribonucleotide -->

    <owl:Class rdf:about="#Ribonucleotide">
        <rdfs:subClassOf rdf:resource="#Nucleotide"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#condensesTo"/>
                <owl:someValuesFrom rdf:resource="#RibonucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#RibonucleotideResidue -->

    <owl:Class rdf:about="#RibonucleotideResidue">
        <rdfs:label xml:lang="en">ribonucleotide</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#BackbonePhosphateUnit"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#HeterocyclicBaseGroup"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#RiboseRing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#BackbonePhosphateUnit"/>
                                    <rdf:Description rdf:about="#HeterocyclicBaseGroup"/>
                                    <rdf:Description rdf:about="#RiboseRing"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#NucleotideResidue"/>
    </owl:Class>
    


    <!-- http://purl.org/chemtop/dev#Ribose -->

    <owl:Class rdf:about="#Ribose">
