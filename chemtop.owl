        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#HydroxyGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 C6Chain</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Hydrocarbon -->

    <owl:Class rdf:about="#Hydrocarbon">
        <rdfs:label>hydrocarbon</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#OrganicMolecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasProperPart"/>
                        <owl:someValuesFrom rdf:resource="#CarbonAtom"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasProperPart"/>
                        <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#CarbonAtom"/>
                                    <rdf:Description rdf:about="#CarbonHydrogenSubstructure"/>
                                    <rdf:Description rdf:about="#HydrogenAtom"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: Compounds consisting of carbon and hydrogen only.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#HydrogenAtom -->

    <owl:Class rdf:about="#HydrogenAtom">
        <rdfs:label>hydrogen atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#HydroxyGroup -->

    <owl:Class rdf:about="#HydroxyGroup">
        <rdfs:label>hydroxy group</rdfs:label>
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#OxygenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#HydrogenAtom"/>
                            <rdf:Description rdf:about="#OxygenAtom"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: OH or OH2+</rdfs:comment>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 OxygenAtom and hasComponentPart exactly 1 HydrogenAtom and hasComponentPart only (OxygenAtom or HydrogenAtom)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#InorganicMolecule -->

    <owl:Class rdf:about="#InorganicMolecule">
        <rdfs:label
            >inorganic molecular entity</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Molecule"/>
                    <owl:Class>
                        <owl:complementOf>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#hasProperPart"/>
                                <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
                            </owl:Restriction>
                        </owl:complementOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: Substances which do not include carbon and its derivatives as their principal elements. However, carbides, carbonates, cyanides, cyanates and carbon disulfide are included in this class.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Lipid -->

    <owl:Class rdf:about="#Lipid">
        <rdfs:label>lipid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:comment
            >Definition: Fat-soluble (lipophilic), naturally-occurring molecule.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#MaterialEntity -->

    <owl:Class rdf:about="#MaterialEntity"/>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#MolecularGroup -->

    <owl:Class rdf:about="#MolecularGroup">
        <rdfs:label>molecular group</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:comment
            >Definition: Linked collection of atoms within a molcular entity. They cannot exist by themselves.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#MolecularPart -->

    <owl:Class rdf:about="#MolecularPart">
        <rdfs:label>molecular part</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MaterialEntity"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#MolecularPartBoundary -->

    <owl:Class rdf:about="#MolecularPartBoundary">
        <rdfs:subClassOf rdf:resource="#PhysicalBoundary"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#boundaryOf"/>
                <owl:someValuesFrom rdf:resource="#MolecularPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: An independent continuant [snap:IndependentContinuant] that is a lower dimensional part of a molecule, normally a closed two-dimensional surface. Boundaries are those privileged parts of molecular parts that exist where two molecular parts meet. Typically molecular part boundaries are located between bound atoms .</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Molecule -->

    <owl:Class rdf:about="#Molecule">
        <rdfs:label>molecular</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MaterialEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#Atom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#MoleculeComplex"/>
        <rdfs:comment
            >Definition: A molecular entity is a self connected compound of two atoms or more which exhibit at least one covalent bound. Molecular entities can be 

 Separately distinguishable molecular structure, neither part of nor attached to any other molecular structure. Atoms are not considered molecular structures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#MoleculeBoundary -->

    <owl:Class rdf:about="#MoleculeBoundary">
        <rdfs:subClassOf rdf:resource="#PhysicalBoundary"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#MoleculeComplex -->

    <owl:Class rdf:about="#MoleculeComplex">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MaterialEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:onClass rdf:resource="#Molecule"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: An object which contains several molecules, has direct parts (components) of different types and which changes its ontological nature if subdivided. It must have clear unity and identity criteria.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Monosaccharide -->

    <owl:Class rdf:about="#Monosaccharide">
        <rdfs:label>monosaccharide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Carbohydrate"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#condensesTo"/>
                <owl:someValuesFrom rdf:resource="#SaccharideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Neutron -->

    <owl:Class rdf:about="#Neutron">
        <rdfs:label>neutron</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SubatomicParticle"/>
        <rdfs:comment
            >Definition: Neutral Particle</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#NitrogenAtom -->

    <owl:Class rdf:about="#NitrogenAtom">
        <rdfs:label>nitrogen atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#NonSteroidLipid -->

    <owl:Class rdf:about="#NonSteroidLipid">
        <rdfs:label
            >non steroid lipid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Lipid"/>
        <rdfs:comment
            >Definition: Lipids that have aliphatic chains and no steroid rings</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#NucleicAcidMolecule -->

    <owl:Class rdf:about="#NucleicAcidMolecule">
        <rdfs:label
            >sequence of nucleotides</rdfs:label>
        <rdfs:label>nucleic acid</rdfs:label>
        <rdfs:label
            >nucleotide sequence</rdfs:label>
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#NucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: A nucleic acid is a complex, high-molecular-weight biochemical macromolecule composed of nucleotide chains that convey genetic information.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#NucleicAcidRegion -->

    <owl:Class rdf:about="#NucleicAcidRegion">
        <rdfs:label
            >nucleic acid region</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#NucleosidylGroup -->

    <owl:Class rdf:about="#NucleosidylGroup">
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HeterocyclicBaseGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#SugarRing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Nucleotide -->

    <owl:Class rdf:about="#Nucleotide">
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#PhosphateGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#condensesTo"/>
                <owl:someValuesFrom rdf:resource="#NucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HeterocyclicBaseGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#SugarRing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#NucleotideResidue -->

    <owl:Class rdf:about="#NucleotideResidue">
        <rdfs:label>nucleotide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Residue"/>
