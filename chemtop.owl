                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#BackbonePhosphateUnit"/>
                            <rdf:Description rdf:about="#HeterocyclicBaseGroup"/>
                            <rdf:Description rdf:about="#SugarRing"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 HeterocyclicBase and hasComponentPart exactly 1 Phosphate and hasComponentPart only (Phosphate or Ribose or HeterocyclicBase) and hasComponentPart exactly 1 Pentose</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Oligomer -->

    <owl:Class rdf:about="#Oligomer">
        <rdfs:label
            >oligo or polymer</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#OrganicMolecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom rdf:resource="#Residue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:onClass rdf:resource="#Residue"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: A Oligomer is a molecule of low relative molecular mass of which the components can be fully specified. The components are all of the same kind, e.g. amino acid residues or nucleotide residues.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#OligopeptideMolecule -->

    <owl:Class rdf:about="#OligopeptideMolecule">
        <rdfs:label
            >entire oligopeptide molecule</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Peptide"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:onClass rdf:resource="#AminoAcidResidue"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:allValuesFrom rdf:resource="#AminoAcidResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart max 10 AminoAcidMonomer</rdfs:comment>
        <rdfs:comment
            >Definition: Amino Acid Polymer up to 10 monomers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Oligosaccharide -->

    <owl:Class rdf:about="#Oligosaccharide">
        <rdfs:label>oligosaccharide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Carbohydrate"/>
        <rdfs:comment
            >(QCR) Carbohydrate and hasComponentPart min 2 Monosaccharide and hasComponentPart max 9 Monosaccharide and hasComponentPart only Monomer</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#OrganicMolecule -->

    <owl:Class rdf:about="#OrganicMolecule">
        <rdfs:label
            >organic molecular entity</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Molecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasProperPart"/>
                        <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: Substances containing carbon and its derivatives as their prinicipal elements, usually combined with itself and with atoms of one or more other element such as hydrogen, oxygen, nitrogen, sulfur, phosphorus, chlorine or fluorine. They exist in either carbon chain or carbon ring form. Excludes carbides, carbonates and carbon oxides.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#OxygenAtom -->

    <owl:Class rdf:about="#OxygenAtom">
        <rdfs:label>oxygen atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Pentose -->

    <owl:Class rdf:about="#Pentose">
        <rdfs:label>pentose</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Monosaccharide"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#HydroxyGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >(QCR) hasComponentPart exactly 1 C5Chain</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Peptide -->

    <owl:Class rdf:about="#Peptide">
        <rdfs:label>peptide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#OrganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >We here use the term peptide in the most general sense, viz. a oligo or polymer made out of amino acids.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PeptideBoundary -->

    <owl:Class rdf:about="#PeptideBoundary">
        <rdfs:subClassOf rdf:resource="#MolecularPartBoundary"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PhosphateGroup -->

    <owl:Class rdf:about="#PhosphateGroup">
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Phospholipid -->

    <owl:Class rdf:about="#Phospholipid">
        <rdfs:subClassOf rdf:resource="#Lipid"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PhosphorusAtom -->

    <owl:Class rdf:about="#PhosphorusAtom">
        <rdfs:label>phosphorus atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PhysicalBoundary -->

    <owl:Class rdf:about="#PhysicalBoundary">
        <rdfs:comment
            >Definition: A independent continuant entity that bounds or demarcates some other continuant entity. Commonly viewed as a lower-dimensional part, the the notion of physical boundary here also encompasses ill-delimited spaces that fulfill the same function, e.g. biological joints.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Polymer -->

    <owl:Class rdf:about="#Polymer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#OrganicMolecule"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:allValuesFrom rdf:resource="#Residue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:onClass rdf:resource="#Residue"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PolypeptideMolecule -->

    <owl:Class rdf:about="#PolypeptideMolecule">
        <rdfs:label
            >entire polypeptide molecule</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Peptide"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:allValuesFrom rdf:resource="#AminoAcidResidue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGranularPart"/>
                        <owl:onClass rdf:resource="#AminoAcidResidue"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: Amino Acid Polymer from 11 to 100 monomers.</rdfs:comment>
        <rdfs:comment
            >(QCR) Peptide and hasComponentPart min 11 AminoAcidMonomer and not (ro:properPartOf some MolecularEntity) and hasComponentPart only AminoAcidMonomer and hasComponentPart max 100 AminoAcidMonomer</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Polysaccharide -->

    <owl:Class rdf:about="#Polysaccharide">
        <rdfs:label>polysaccharide</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Carbohydrate"/>
        <rdfs:comment
            >(QCR) Carbohydrate and hasComponentPart min 10 Monosaccharide and hasComponentPart only Monomer</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PortionOfHeterogeneousLiquid -->

    <owl:Class rdf:about="#PortionOfHeterogeneousLiquid">
        <rdfs:label
            >portion of heterogenous liquid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CompoundOfCollectiveMaterialEntities"/>
        <rdfs:comment
            >Definition: Liquid constituted by different kinds of collectives, e.g. aqueous NaCl solution</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PortionOfHeterogeneousSolid -->

    <owl:Class rdf:about="#PortionOfHeterogeneousSolid">
        <rdfs:label
            >portion of heterogenous solid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CompoundOfCollectiveMaterialEntities"/>
        <rdfs:comment
            >Definition: Solid constituted by different kinds of collectives, e.g. a NaCl crystal, granite</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PortionOfHomogeneousGas -->

    <owl:Class rdf:about="#PortionOfHomogeneousGas">
        <rdfs:label>portion of gas</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PortionOfHomogeneousMatter"/>
        <rdfs:comment
            >Definition: Non-connected Molecules in gaseous state</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PortionOfHomogeneousLiquid -->

    <owl:Class rdf:about="#PortionOfHomogeneousLiquid">
        <rdfs:label
            >portion of homogenous liquid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PortionOfHomogeneousMatter"/>
        <rdfs:comment
            >Definition: Pure liquid consisting only of one kind of particles, e.g. H2O, liquid Hg, ...</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PortionOfHomogeneousMatter -->

    <owl:Class rdf:about="#PortionOfHomogeneousMatter">
        <rdfs:label
            >homogenous cohesive collection</rdfs:label>
        <rdfs:subClassOf rdf:resource="#CollectiveMaterialEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasGranularPart"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Atom"/>
                            <rdf:Description rdf:about="#Molecule"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasGranularPart"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Atom"/>
                            <rdf:Description rdf:about="#Molecule"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: Composition of granular components of the same kind.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PortionOfHomogeneousSolid -->

    <owl:Class rdf:about="#PortionOfHomogeneousSolid">
        <rdfs:label
            >portion of homogenous solid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PortionOfHomogeneousMatter"/>
        <rdfs:comment
            >Definition: Pure solid consisting only of one kind of particles, e.g. a piece of metal</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#ProteinComplex -->

    <owl:Class rdf:about="#ProteinComplex">
        <rdfs:label>protein complex</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MoleculeComplex"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:onClass rdf:resource="#ProteinSubunit"/>
                        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment
            >Definition: Mostly only proteins but composition with other molecules possible (e.g. Fe -&gt; Hemoglobin)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#ProteinDomain -->

    <owl:Class rdf:about="#ProteinDomain">
        <rdfs:label>protein domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidChain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#partOf"/>
                <owl:someValuesFrom rdf:resource="#ProteinMolecule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: Structural domain is the smallest element of protein tertiary structure that is self-stabilizing and often folds independently of the rest of the amino acid chain. It may contain one or more structural motives (ProteinSubstructure). A protein domain typically has its own function, the overall function of the protein results from the composition of the functions of all of its structural domains.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#ProteinMolecule -->

    <owl:Class rdf:about="#ProteinMolecule">
        <rdfs:label
            >entire protein molecule</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PolypeptideMolecule"/>
        <rdfs:comment
            >Definition: Amino Acid Polymer longer than 100 monomers.</rdfs:comment>
        <rdfs:comment
            >(QCR) Peptide and hasComponentPart min 101 AminoAcidMonomer and not (ro:properPartOf some MolecularEntity)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#ProteinSubstructure -->

    <owl:Class rdf:about="#ProteinSubstructure">
        <rdfs:label
            >protein substructure</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidChain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#partOf"/>
                <owl:someValuesFrom rdf:resource="#ProteinMolecule"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: A secondary substructure of a protein (protein Motif)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#ProteinSubunit -->

    <owl:Class rdf:about="#ProteinSubunit">
        <rdfs:label>protein subunit</rdfs:label>
        <rdfs:subClassOf rdf:resource="#ProteinMolecule"/>
        <rdfs:comment
            >Definition: A protein molecule that assembles with other protein molecules to form a protein complex. May have its own function. However, most proteinsubunits do not gain functionality until associated with the other subunits.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Proton -->

    <owl:Class rdf:about="#Proton">
        <rdfs:label>proton</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SubatomicParticle"/>
        <rdfs:comment
            >Definition: Positively Charged Particle</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PurineGroup -->

    <owl:Class rdf:about="#PurineGroup">
        <rdfs:subClassOf rdf:resource="#HeterocyclicBaseGroup"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#PyrimidineGroup -->

    <owl:Class rdf:about="#PyrimidineGroup">
        <rdfs:subClassOf rdf:resource="#HeterocyclicBaseGroup"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#RNAMolecule -->

    <owl:Class rdf:about="#RNAMolecule">
        <rdfs:label>RNA</rdfs:label>
        <rdfs:label>RNA sequence</rdfs:label>
        <rdfs:subClassOf rdf:resource="#NucleicAcidMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#RibonucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Definition: Any kind of NucleicAcid that is composed of Ribonucleotides which are not Deoxyribonucleotides</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#RNARegion -->

    <owl:Class rdf:about="#RNARegion">
        <rdfs:label>RNA region</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#RibonucleotideResidue"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#properPartOf"/>
                        <owl:someValuesFrom rdf:resource="#RNAMolecule"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom rdf:resource="#RibonucleotideResidue"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#NucleicAcidRegion"/>
        <rdfs:comment
            >Definition: Any properPartOf an EntireRNAMolecule that as only ribonucleotides as components.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Residue -->

    <owl:Class rdf:about="#Residue">
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Ribonucleotide -->

    <owl:Class rdf:about="#Ribonucleotide">
        <rdfs:subClassOf rdf:resource="#Nucleotide"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#condensesTo"/>
                <owl:someValuesFrom rdf:resource="#RibonucleotideResidue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#RibonucleotideResidue -->

    <owl:Class rdf:about="#RibonucleotideResidue">
        <rdfs:label>ribonucleotide</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#BackbonePhosphateUnit"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#HeterocyclicBaseGroup"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:someValuesFrom rdf:resource="#RiboseRing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponentPart"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#BackbonePhosphateUnit"/>
                                    <rdf:Description rdf:about="#HeterocyclicBaseGroup"/>
                                    <rdf:Description rdf:about="#RiboseRing"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#NucleotideResidue"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Ribose -->

    <owl:Class rdf:about="#Ribose">
        <rdfs:subClassOf rdf:resource="#Pentose"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#RiboseRing -->

    <owl:Class rdf:about="#RiboseRing">
        <rdfs:label>ribose</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SugarRing"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#SaccharideResidue -->

    <owl:Class rdf:about="#SaccharideResidue">
        <rdfs:subClassOf rdf:resource="#Residue"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Steroid -->

    <owl:Class rdf:about="#Steroid">
        <rdfs:label>steroid</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Lipid"/>
        <rdfs:comment
            >Definition: Terpenoid lipid characterized by a carbon skeleton with four fused rings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#StructuralGroup -->

    <owl:Class rdf:about="#StructuralGroup">
        <rdfs:label
            >structural group</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MolecularGroup"/>
        <rdfs:comment
            >Definition: Structural groups are specific constellations of atoms within molecules which are relevant for the classification of those compounds, e.g. Pentoses have a 5-C-Chain, Hexoses have a 6-C-Chain.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#SubatomicParticle -->

    <owl:Class rdf:about="#SubatomicParticle">
        <rdfs:label
            >subatomic particle</rdfs:label>
        <rdfs:subClassOf rdf:resource="#MaterialEntity"/>
        <rdfs:comment
            >Definition: Parts of Atoms.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#SugarRing -->

    <owl:Class rdf:about="#SugarRing">
        <rdfs:subClassOf rdf:resource="#MolecularPart"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#SulphurAtom -->

    <owl:Class rdf:about="#SulphurAtom">
        <rdfs:label>sulphur atom</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Atom"/>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Thymin-1-yl_group -->

    <owl:Class rdf:about="#Thymin-1-yl_group">
        <rdfs:label>thymin</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PyrimidineGroup"/>
        <rdfs:comment
            >Synonym: C5H6N2O2</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#Uracil-1-yl_group -->

    <owl:Class rdf:about="#Uracil-1-yl_group">
        <rdfs:label>uracil</rdfs:label>
        <rdfs:subClassOf rdf:resource="#PyrimidineGroup"/>
        <rdfs:comment
            >Synonym: C4H4N2O2</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/biotop/1.0/chemtop.owl#WaterMolecule -->

    <owl:Class rdf:about="#WaterMolecule">
        <rdfs:label>water molecule</rdfs:label>
        <rdfs:subClassOf rdf:resource="#InorganicMolecule"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponentPart"/>
                <owl:someValuesFrom rdf:resource="#OxygenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
