            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom rdf:resource="&snap;IndependentContinuant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#PolyatomicEntity"/>
        <rdfs:label xml:lang="en"
            >poly molecular compound</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: An non-molecular object which has direct parts (components) of different types and which changes its ontological nature if subdivided.</rdfs:comment>
    </owl:Class>
