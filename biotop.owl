                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponent"/>
                        <owl:someValuesFrom rdf:resource="#EntireMolecularEntity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#PolyMolecularCompound"/>
        <owl:disjointWith rdf:resource="#Organism"/>
        <rdfs:label xml:lang="en"
            >molecule complex</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Particle that consists of entire molecules.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="Monomer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
