                                    </owl:Restriction>
                                    <owl:Class>
                                        <owl:complementOf>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <owl:Class rdf:about="#MolecularEntity"/>
                                                    <owl:Class rdf:about="#Organism"/>
                                                    <owl:Class rdf:about="#OrganismPart"/>
                                                </owl:unionOf>
                                            </owl:Class>
                                        </owl:complementOf>
                                    </owl:Class>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#HumanAction"/>
