    <owl:Class rdf:ID="ProkaryoticCell">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class rdf:about="#ArchaebacterialCell"/>
                    <owl:Class rdf:about="#BacterialCell"/>
