                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#MachineAction"/>
    </owl:Class>
    <owl:Class rdf:ID="Hydrocarbon">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponent"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Class rdf:about="#CarbonAtom"/>
                                    <owl:Class rdf:about="#CarbonHydrogenSubstructure"/>
                                    <owl:Class rdf:about="#HydrogenAtom"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                    <owl:Class rdf:about="#MolecularEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;hasProperPart"/>
                        <owl:someValuesFrom rdf:resource="#CarbonAtom"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;hasProperPart"/>
                        <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">hydrocarbon</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Compounds consisting of carbon and hydrogen only.</rdfs:comment>
