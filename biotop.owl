                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#hasComponent"/>
                                <owl:someValuesFrom rdf:resource="#CarboxyGroup"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Class rdf:about="#MolecularEntity"/>
