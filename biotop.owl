                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class rdf:about="#Eukaryote"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&rol;properPartOf"/>
                                <owl:someValuesFrom rdf:resource="#Eukaryote"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">eukaryotic cell</rdfs:label>
    </owl:Class>
    <owl:Class rdf:ID="FattyAcid">
