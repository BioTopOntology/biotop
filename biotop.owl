    <owl:Class rdf:ID="HomogenousCohesiveCollection">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasGrain"/>
                <owl:someValuesFrom rdf:resource="&snap;IndependentContinuant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#CohesiveCollection"/>
        <owl:disjointWith rdf:resource="#HeterogenousCohesiveCollection"/>
        <rdfs:label xml:lang="en"
            >homogenous cohesive collection</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Composition of connected granular components of the same kind.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="Human">
        <rdfs:subClassOf rdf:resource="#Animal"/>
    </owl:Class>
