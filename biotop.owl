                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Class rdf:about="#Atom"/>
                                    <owl:Class rdf:about="#MolecularGroup"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponent"/>
