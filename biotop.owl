        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rol;hasPart"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidSequence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#Peptide"/>
        <rdfs:label xml:lang="en"
            >protein substructure</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: A secondary substructure of a protein (protein Motif)</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="ProteinSubunit">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#componentOf"/>
