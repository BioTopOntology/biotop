                        <owl:someValuesFrom rdf:resource="#Peptide"/>
                    </owl:Restriction>
                    <owl:Class rdf:about="#MolecularFunction"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en"
            >protein function</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Inherent Function of a protein or a part of it corresponds to GO molecular function.</rdfs:comment>
    </owl:Class>
