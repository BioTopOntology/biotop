                        <owl:onProperty rdf:resource="&rol;hasAgent"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="#hasInherence"/>
