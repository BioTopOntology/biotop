        <rdfs:domain rdf:resource="#ObjectOrFiatObjectPart"/>
        <rdfs:range rdf:resource="&snap;IndependentContinuant"/>
        <rdfs:subPropertyOf rdf:resource="&rol;hasProperPart"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment:  inverse of componentOf.</rdfs:comment>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="hasGrain">
        <rdfs:domain rdf:resource="#CollectiveObject"/>
        <rdfs:range rdf:resource="&snap;IndependentContinuant"/>
        <rdfs:subPropertyOf rdf:resource="&rol;hasProperPart"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment:  inverse of grainOf.</rdfs:comment>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="hasInherence">
        <rdfs:domain rdf:resource="&snap;IndependentContinuant"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Inverse relation of inheresIn. Relates an independent continuant to a dependent one (e.g. quality, state, function, or disposition)</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >For performance reasons, inverse relations are not encoded as such in OWL.</rdfs:comment>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="hasPatient">
        <rdfs:subPropertyOf rdf:resource="&rol;hasParticipant"/>
    </owl:ObjectProperty>
