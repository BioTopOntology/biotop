<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY bfo "http://www.ifomis.org/bfo/1.1#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY snap "http://www.ifomis.org/bfo/1.1/snap#" >
    <!ENTITY span "http://www.ifomis.org/bfo/1.1/span#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rol "http://www.ifomis.org/obo/ro/1.0/light#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
    <!ENTITY protege-dc "http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#" >
]>


<rdf:RDF xmlns="http://www.ifomis.org/biotop/1.0#"
     xml:base="http://www.ifomis.org/biotop/1.0"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:protege-dc="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:snap="http://www.ifomis.org/bfo/1.1/snap#"
     xmlns:bfo="http://www.ifomis.org/bfo/1.1#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rol="http://www.ifomis.org/obo/ro/1.0/light#"
     xmlns:span="http://www.ifomis.org/bfo/1.1/span#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo rdf:datatype="&xsd;string">1.0</owl:versionInfo>
        <dc:title rdf:datatype="&xsd;string">BioTop</dc:title>
        <dc:creator rdf:datatype="&xsd;string"
            >Holger Stenzhorn</dc:creator>
        <dc:creator rdf:datatype="&xsd;string">Stefan Schulz</dc:creator>
        <dc:creator rdf:datatype="&xsd;string"
            >Elena Beisswanger</dc:creator>
        <dc:publisher rdf:datatype="&xsd;string"
            >University Hospital Freiburg - Medical Informatics Department</dc:publisher>
        <dc:publisher rdf:datatype="&xsd;string"
            >Jena University Language and Information Engineering (JULIE) Lab</dc:publisher>
        <dc:publisher rdf:datatype="&xsd;string"
            >Institute for Formal Ontology and Medical Information Science (IFOMIS)</dc:publisher>
        <dc:date rdf:datatype="&xsd;date">2007-11-05</dc:date>
        <dc:format rdf:datatype="&xsd;string"
            >application/rdf+xml</dc:format>
        <dc:identifier rdf:datatype="&xsd;string"
            >http://www.ifomis.org/biotop/1.0</dc:identifier>
        <dc:source rdf:datatype="&xsd;string"
            >Stefan Schulz, Elena Beisswanger, Joachim Wermter and Udo Hahn: &quot;Towards an Upper Level Ontology for Molecular Biology&quot;</dc:source>
        <dc:source rdf:datatype="&xsd;string"
            >Stefan Schulz, Elena Beisswanger, Udo Hahn, Joachim Wermter, Holger Stenzhorn and Anand Kumar: &quot;From GENIA to BioTop - Towards a top-level Ontology for Biology&quot;</dc:source>
        <dc:language rdf:datatype="&xsd;string">en</dc:language>
        <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/protege"/>
        <owl:imports rdf:resource="http://www.ifomis.org/bfo/1.1"/>
        <owl:imports rdf:resource="http://www.ifomis.org/obo/ro/1.0/light"/>
    </owl:Ontology>
    <owl:AllDifferent>
        <owl:distinctMembers rdf:parseType="Collection"/>
    </owl:AllDifferent>
    <owl:Class rdf:ID="AcquiredPathologicalState">
        <rdfs:subClassOf rdf:resource="#PathologicalState"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Ill-formed state of a biological entity,acquired after birth [steschu 20071010]</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="Action">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;hasAgent"/>
                        <owl:someValuesFrom rdf:resource="&snap;IndependentContinuant"/>
                    </owl:Restriction>
                    <owl:Class rdf:about="&span;Process"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="&xsd;string"
            >An action is a processual entity that that is promoted by an agent.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="Adenine">
        <rdfs:subClassOf rdf:resource="#HeterocyclicBase"/>
        <owl:disjointWith rdf:resource="#Cytosine"/>
        <owl:disjointWith rdf:resource="#Guanine"/>
        <owl:disjointWith rdf:resource="#Thymin"/>
        <owl:disjointWith rdf:resource="#Uracil"/>
        <rdfs:label xml:lang="en">adenine</rdfs:label>
    </owl:Class>
    <owl:Class rdf:ID="AliphaticChain">
        <rdfs:subClassOf rdf:resource="#Hydrocarbon"/>
        <owl:disjointWith rdf:resource="#CyclicAliphatic"/>
        <owl:disjointWith rdf:resource="#AromaticHydrocarbon"/>
        <rdfs:label xml:lang="en">aliphatic chain</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Hydrocarbon without rings.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="AliphaticMediumChainStructure">
        <rdfs:subClassOf rdf:resource="#AliphaticChain"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rol;hasProperPart"/>
                <owl:someValuesFrom rdf:resource="#C10-24Chain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en"
            >aliphatic medium chain structure</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Hydrophobic Chain (length generally 10 - 24 C atoms), such as constituting the &quot;tail&quot; of a fatty acid. May contain unsaturated segments.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="AminoAcidMonomer">
        <rdfs:subClassOf rdf:resource="#MolecularEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom rdf:resource="#CarboxyGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&rol;hasPart"/>
                <owl:someValuesFrom rdf:resource="#CarbonHydrogenSubstructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom rdf:resource="#AminoGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Nucleotide"/>
        <owl:disjointWith rdf:resource="#HeterocyclicBase"/>
        <rdfs:label xml:lang="en"
            >amino acid monomer</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) ro:hasPart some CarbonHydrogenSubstructure and ((hasComponent myx 2 CarboxyGroup) and (hasComponent min 1 CarboxyGroup) and (hasComponent exactly 1 AminoGroup)) or ((hasComponent max 2 AminoGroup) and (hasComponent min 1 AminoGroup) and (hasComponent exactly 1 CarboxyGroup))</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: In chemistry, an amino acid is any molecule that contains both amine and carboxyl functional groups. However, in biochemistry, this more general term is frequently used to refer to alpha amino acids (amino acids in which the amino and carboxylate functionalities are attached to the same carbon, called &#945;&#8211;carbon). Amino Acids have at least one amino and one carboxy group. Those having two carboxy groups have only one amino group and vice versa.</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Amino Acids molecules or residues (residues as in peptide bonds)</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="AminoAcidSequence">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponent"/>
                        <owl:allValuesFrom rdf:resource="#AminoAcidMonomer"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponent"/>
                        <owl:someValuesFrom rdf:resource="#AminoAcidMonomer"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Peptide"/>
        <owl:disjointWith rdf:resource="#Carbohydrate"/>
        <rdfs:label xml:lang="en"
            >amino acid sequence</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Synonym: AminoAcidSequence</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Chain of amino acids, joined by peptide bonds. Folded or unfolded.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="AminoGroup">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class rdf:about="#HydrogenAtom"/>
                            <owl:Class rdf:about="#NitrogenAtom"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom rdf:resource="#HydrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#FunctionalGroup"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom rdf:resource="#NitrogenAtom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#C5Chain"/>
        <owl:disjointWith rdf:resource="#CarbonHydrogenSubstructure"/>
        <owl:disjointWith rdf:resource="#HydroxyGroup"/>
        <owl:disjointWith rdf:resource="#CarboxyGroup"/>
        <owl:disjointWith rdf:resource="#CarbonHydrogenHydroxySubstructure"/>
        <owl:disjointWith rdf:resource="#C6Chain"/>
        <rdfs:label xml:lang="en">amino group</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Amines are organic compounds and a type of functional group that contain nitrogen as the key atom. Isolated neutral or kationic amino group or amino group forming a peptide bond. NH2 or NH3+</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) hasComponent exactly 1 NitrogenAtom and hasComponent max 3 HydrogenAtom and hasComponent min 1 HydrogenAtom and hasComponent only (NitrogenAtom or HydrogenAtom)</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="AnatomicalStructure">
        <rdfs:subClassOf rdf:resource="#MaterialAnatomicalEntity"/>
        <rdfs:label xml:lang="en"
            >anatomical structure</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >FMA: Material anatomical entity which is generated by coordinated expression of the organism&apos;s own genes that guide its morphogenesis; has inherent 3D shape; its parts are connected and spatially related to one another in patterns determined by coordinated gene expression. Examples: heart, right ventricle, mitral valve, myocardium, endothelium, lymphocyte, fibroblast, thorax, cardiovascular system, hemoglobin, T cell receptor.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="Animal">
        <rdfs:subClassOf rdf:resource="#Eukaryote"/>
        <owl:disjointWith rdf:resource="#Fungus"/>
        <owl:disjointWith rdf:resource="#Plant"/>
        <owl:disjointWith rdf:resource="#Protist"/>
        <owl:disjointWith rdf:resource="#ModifiedCell"/>
        <rdfs:label xml:lang="en">animal</rdfs:label>
    </owl:Class>
    <owl:Class rdf:ID="ArchaebacterialCell">
        <rdfs:subClassOf rdf:resource="#ProkaryoticCell"/>
        <owl:disjointWith rdf:resource="#BacterialCell"/>
        <rdfs:label xml:lang="en"
            >archaebacterial cell</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Synonym: Archaebacterium</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: We conceive organisms in a material sense. Therefore we do not assume any ontological difference between a monocellular organism and the cell it is made of.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="AromaticHydrocarbon">
        <rdfs:subClassOf rdf:resource="#Hydrocarbon"/>
        <owl:disjointWith rdf:resource="#AliphaticChain"/>
        <owl:disjointWith rdf:resource="#CyclicAliphatic"/>
        <rdfs:label xml:lang="en"
            >aromatic hydrocarbon</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Hydrocarbons which contain aromatic rings (benzene, naphtalin, toluene)</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="ArtefactRole">
        <rdfs:subClassOf rdf:resource="&snap;Role"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The role an entity plays when it has been artificially produced. [steschu 20071010]</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="Atom">
        <rdfs:subClassOf rdf:resource="#ObjectOrFiatObjectPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:allValuesFrom rdf:resource="#SubatomicParticle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom rdf:resource="#SubatomicParticle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#SubatomicParticle"/>
        <owl:disjointWith rdf:resource="#MolecularEntity"/>
        <rdfs:label xml:lang="en">atom</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Basic building block of molecules. Particle of discrete types. Biologically important atoms are C, N, H, O, P, S, Na, Ca, K, Mg, Cl, J, Fe, Co, Zn ,... Atoms may be electrically neutral or ionic.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="BacterialCell">
        <rdfs:subClassOf rdf:resource="#ProkaryoticCell"/>
        <owl:disjointWith rdf:resource="#ArchaebacterialCell"/>
        <rdfs:label xml:lang="en">bacterial cell</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Synonym: Bacterium</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: We conceive organisms in a material sense. Therefore we do not assume any ontological difference between a monocellular organism and the cell it is made of.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="BacterialColony">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class rdf:about="#BiologicalColony"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGrain"/>
                        <owl:someValuesFrom rdf:resource="#BacterialCell"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Cluster of bacteria , usually cultured from a single cell.</rdfs:comment>
    </owl:Class>
    <rdf:Description rdf:about="&bfo;Entity"/>
    <owl:Class rdf:ID="BiologicalColony">
        <rdfs:subClassOf rdf:resource="#PluralityOfOrganisms"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Plurality of several individual organisms living closely together, usually for mutual benefit</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="BiologicalProcess">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;hasParticipant"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Class rdf:about="#Organism"/>
                                    <owl:Class rdf:about="#OrganismPart"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;locatedIn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Class rdf:about="#Organism"/>
                                    <owl:Class rdf:about="#OrganismPart"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Class rdf:about="&span;Process"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en"
            >biological process</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Process specific for biological systems.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="BioMolecularProcess">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class rdf:about="#BiologicalProcess"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;hasParticipant"/>
                        <owl:someValuesFrom rdf:resource="#MolecularEntity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en"
            >molecular process</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Process occuring on molecular level. It has exclusivly subclasses of MolecularEntity as participants.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="BioMolecularSequence">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class rdf:about="#AminoAcidSequence"/>
                    <owl:Class rdf:about="#DNA"/>
                    <owl:Class rdf:about="#RNA"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#MolecularEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasInherence"/>
                <owl:someValuesFrom rdf:resource="#BioMolecularSequenceInformation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    <owl:Class rdf:ID="BioMolecularSequenceInformation">
        <rdfs:subClassOf rdf:resource="#NonRealizableInformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#inheresIn"/>
                <owl:someValuesFrom rdf:resource="#BioMolecularSequence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Sequence as an entity of information. The same sequence can inhere in different biopolymers.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="BodyFluid">
        <rdfs:subClassOf rdf:resource="#PortionOfHeterogenousLiquid"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGrain"/>
                        <owl:someValuesFrom rdf:resource="#MolecularEntity"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasComponent"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGrain"/>
                        <owl:someValuesFrom rdf:resource="#WaterMolecule"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;derivesFrom"/>
                        <owl:someValuesFrom rdf:resource="#Organism"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&rol;partOf"/>
                        <owl:someValuesFrom rdf:resource="#Organism"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">body fluid</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Suspension and / or solution of biomolecules, ions, bigger particles in Water.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="C10-24Chain">
        <rdfs:subClassOf rdf:resource="#CarbonChain"/>
        <rdfs:label xml:lang="en">C10-24 chain</rdfs:label>
    </owl:Class>
    <owl:Class rdf:ID="C5Chain">
        <rdfs:subClassOf rdf:resource="#CarbonChain"/>
        <owl:disjointWith rdf:resource="#CarbonHydrogenHydroxySubstructure"/>
        <owl:disjointWith rdf:resource="#CarboxyGroup"/>
        <owl:disjointWith rdf:resource="#HydroxyGroup"/>
        <owl:disjointWith rdf:resource="#AminoGroup"/>
        <owl:disjointWith rdf:resource="#C6Chain"/>
        <owl:disjointWith rdf:resource="#CarbonHydrogenSubstructure"/>
        <rdfs:label xml:lang="en">C5 chain</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) hasComponent exactly 5 CarbonAtom</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Backbone of Pentoses.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="C6Chain">
        <rdfs:subClassOf rdf:resource="#CarbonChain"/>
        <owl:disjointWith rdf:resource="#AminoGroup"/>
        <owl:disjointWith rdf:resource="#CarbonHydrogenSubstructure"/>
        <owl:disjointWith rdf:resource="#CarboxyGroup"/>
        <owl:disjointWith rdf:resource="#CarbonHydrogenHydroxySubstructure"/>
        <owl:disjointWith rdf:resource="#HydroxyGroup"/>
        <owl:disjointWith rdf:resource="#C5Chain"/>
        <rdfs:label xml:lang="en">C6 chain</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Definition: Backbone of Hexoses.</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comment: (QCR) hasComponent exactly 6 CarbonAtom</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:ID="CanonicalState">
        <rdfs:subClassOf rdf:resource="#StateOfBeing"/>
    </owl:Class>
    <owl:Class rdf:ID="Carbohydrate">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class rdf:about="#Monosaccharide"/>
                    <owl:Class rdf:about="#Oligosaccharide"/>
                    <owl:Class rdf:about="#Polysaccharide"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#MolecularEntity"/>
        <owl:disjointWith rdf:resource="#AminoAcidSequence"/>
        <rdfs:label xml:lang="en">carbohydrate</rdfs:label>
    </owl:Class>
    <owl:Class rdf:ID="CarbonAtom">
        <rdfs:subClassOf rdf:resource="#Atom"/>
        <owl:disjointWith rdf:resource="#PhosphorusAtom"/>
        <owl:disjointWith rdf:resource="#HydrogenAtom"/>
        <owl:disjointWith rdf:resource="#OxygenAtom"/>
        <owl:disjointWith rdf:resource="#SulphurAtom"/>
        <owl:disjointWith rdf:resource="#NitrogenAtom"/>
        <rdfs:label xml:lang="en">carbon atom</rdfs:label>
    </owl:Class>
    <owl:Class rdf:ID="CarbonChain">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasComponent"/>
                        <owl:allValuesFrom rdf:resource="#CarbonAtom"/>
